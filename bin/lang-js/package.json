{
  "name": "lang-js",
  "private": true,
  "version": "0.1.0",
  "description": "Inference for Entities",
  "main": "dist/index.js",
  "bin": {
    "lang-js": "./bin-placeholder.js"
  },
  "pkg": {
    "scripts": "dist/**/*.js",
    "assets": [
      "node_modules/typescript/**/*.ts",
      "node_modules/commander/**/*.js",
      "node_modules/debug/**/*.js",
      "node_modules/vm2/**/*",
      "node_modules/lodash/**/*.js"
    ]
  },
  "scripts": {
    "clean": "rm -rf ./dist ./target",
    "build": "tsc",
    "build:check": "tsc --noEmit --skipLibCheck",
    "build:clean": "pnpm run clean && pnpm run build",
    "build:watch": "pnpm run clean && tsc --watch",
    "package": "pnpm run build && pkg . --output ./target/lang-js",
    "jsbuild:backend": "pnpm package",
    "watch": "pnpm run build:watch",
    "lint": "eslint --ext .ts,.js --ignore-path .gitignore src",
    "lint:fix": "eslint --fix",
    "fmt": "pnpm run lint:fix",
    "fmt:check": "pnpm run lint",
    "check": "pnpm run lint && pnpm run build:check && pnpm run fmt:check",
    "dev": "pnpm run start",
    "start": "pnpm run build && node ./dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch --forceExit",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand"
  },
  "author": "System Initiative, Inc.",
  "license": "Proprietary",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.1",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.15.11",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.5",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.0.0",
    "pkg": "^5.6.0",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.4"
  },
  "dependencies": {
    "@typescript/vfs": "^1.4.0",
    "commander": "^9.2.0",
    "debug": "^4.3.4",
    "execa": "^5.1.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2",
    "typescript": "^4.9.5",
    "vm2": "^3.9.11"
  },
  "volta": {
    "extends": "../../package.json",
    "node": "16.20.0"
  }
}
