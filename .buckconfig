[repositories]
root = .
bxl = bxl
prelude = prelude
prelude-si = prelude-si
toolchains = toolchains
none = none

[repository_aliases]
buck = none
config = prelude
fbcode = none
fbsource = none

[parser]
target_platform_detector_spec = target:root//...->prelude//platforms:default

[project]
ignore = \
  .git, \
  .idea, \
  app/**/node_modules, \
  bin/**/node_modules, \
  lib/**/node_modules, \
  node_modules, \
  target, \
  third-party/rust/.cargo/**/BUCK, \
  third-party/rust/vendor/**/BUCK
